package week4;

/**
 *
 * @author CCNE
 */
public class Cam extends HoaQua{
    protected String color;
    protected String muivi;

    public String getMuivi() {
        return muivi;
    }

    public void setMuivi(String muivi) {
        this.muivi = muivi;
    }
    public String getColor() {
        return color;
    }

    public void setColor(String color) {
        this.color = color;
    }
    public Cam(){
        super();
        this.color="mau cam";
        this.muivi="mui rat thom";
    }
    public Cam( String Gia,String Nguongoc,String Ngaynhap,int Soluong,String color,String muivi){
        super(Gia, Nguongoc, Ngaynhap, Soluong);
       this.color=color;
       this.muivi=muivi;
    }
  
    
}
