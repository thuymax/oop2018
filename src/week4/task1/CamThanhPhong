package week4;

/**
 *
 * @author CCNE
 */
public class CamThanhPhong extends Cam {
    private String kichco;
    private String size;

    public String getKichco() {
        return kichco;
    }

    public void setKichco(String kichco) {
        this.kichco = kichco;
    }

    public String getSize() {
        return size;
    }

    public void setSize(String size) {
        this.size = size;
    }
    public CamThanhPhong(){
        super();
        this.kichco="150cm";
        this.size="medium";
    }
    public CamThanhPhong(String Gia,String Nguongoc,String Ngaynhap,int Soluong,String color,String muivi){
        super(Gia, Nguongoc, Ngaynhap, Soluong, color, muivi);
    }
   
}
